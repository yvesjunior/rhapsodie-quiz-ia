CREATE TABLE `tbl_group_battle` (
  `id` bigint unsigned NOT NULL AUTO_INCREMENT,
  `group_id` int unsigned NOT NULL,
  `topic_id` int unsigned NOT NULL,
  `category_id` int NOT NULL,
  `created_by` int unsigned NOT NULL,
  `title` varchar(200) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `question_count` int NOT NULL DEFAULT '10',
  `time_per_question` int NOT NULL DEFAULT '15',
  `entry_coins` int NOT NULL DEFAULT '0',
  `prize_coins` int NOT NULL DEFAULT '0',
  `status` enum('pending','active','completed','cancelled') COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT 'pending',
  `min_players` int NOT NULL DEFAULT '2',
  `max_players` int NOT NULL DEFAULT '10',
  `player_count` int NOT NULL DEFAULT '0',
  `questions` json DEFAULT NULL,
  `scheduled_start` datetime DEFAULT NULL,
  `started_at` datetime DEFAULT NULL,
  `ended_at` datetime DEFAULT NULL,
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`),
  KEY `idx_battle_group` (`group_id`),
  KEY `idx_battle_topic` (`topic_id`),
  KEY `idx_battle_category` (`category_id`),
  KEY `idx_battle_status` (`status`),
  KEY `idx_battle_scheduled` (`scheduled_start`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
