CREATE TABLE `tbl_battle_1v1` (
  `id` bigint unsigned NOT NULL AUTO_INCREMENT,
  `match_code` varchar(20) COLLATE utf8mb4_unicode_ci NOT NULL,
  `topic_id` int unsigned NOT NULL,
  `category_id` int NOT NULL,
  `challenger_id` int unsigned NOT NULL,
  `opponent_id` int unsigned DEFAULT NULL,
  `question_count` int NOT NULL DEFAULT '10',
  `time_per_question` int NOT NULL DEFAULT '15',
  `entry_coins` int NOT NULL DEFAULT '0',
  `prize_coins` int NOT NULL DEFAULT '0',
  `questions` json DEFAULT NULL,
  `challenger_answers` json DEFAULT NULL,
  `challenger_score` int NOT NULL DEFAULT '0',
  `challenger_correct` int NOT NULL DEFAULT '0',
  `challenger_time_ms` int NOT NULL DEFAULT '0',
  `challenger_ready` tinyint(1) NOT NULL DEFAULT '0',
  `opponent_answers` json DEFAULT NULL,
  `opponent_score` int NOT NULL DEFAULT '0',
  `opponent_correct` int NOT NULL DEFAULT '0',
  `opponent_time_ms` int NOT NULL DEFAULT '0',
  `opponent_ready` tinyint(1) NOT NULL DEFAULT '0',
  `winner_id` int unsigned DEFAULT NULL,
  `is_draw` tinyint(1) NOT NULL DEFAULT '0',
  `status` enum('waiting','ready','playing','completed','expired','cancelled') COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT 'waiting',
  `expires_at` datetime DEFAULT NULL,
  `started_at` datetime DEFAULT NULL,
  `ended_at` datetime DEFAULT NULL,
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`),
  UNIQUE KEY `match_code` (`match_code`),
  KEY `idx_1v1_match_code` (`match_code`),
  KEY `idx_1v1_challenger` (`challenger_id`),
  KEY `idx_1v1_opponent` (`opponent_id`),
  KEY `idx_1v1_topic` (`topic_id`),
  KEY `idx_1v1_category` (`category_id`),
  KEY `idx_1v1_status` (`status`),
  KEY `idx_1v1_winner` (`winner_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
