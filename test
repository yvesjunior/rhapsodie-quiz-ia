--
-- Table structure for table `tbl_category`
--

DROP TABLE IF EXISTS `tbl_category`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!50503 SET character_set_client = utf8mb4 */;
CREATE TABLE `tbl_category` (
  `id` int NOT NULL AUTO_INCREMENT,
  `topic_id` int unsigned DEFAULT NULL,
  `parent_id` int DEFAULT NULL,
  `language_id` int NOT NULL DEFAULT '0',
  `category_name` varchar(250) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NOT NULL,
  `devotional_title` varchar(255) DEFAULT NULL,
  `slug` varchar(250) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin DEFAULT NULL,
  `type` int NOT NULL,
  `category_type` enum('year','month','day','module','general') NOT NULL DEFAULT 'general',
  `age_group` enum('kids','teens','adults','all') NOT NULL DEFAULT 'all',
  `is_premium` tinyint NOT NULL DEFAULT '0' COMMENT '0 - no , 1 - yes',
  `coins` int NOT NULL DEFAULT '0',
  `image` text CHARACTER SET utf8mb4 COLLATE utf8mb4_bin,
  `row_order` int NOT NULL,
  `year` int DEFAULT NULL,
  `month` int DEFAULT NULL,
  `day` int DEFAULT NULL,
  `daily_text` text,
  `scripture_ref` varchar(100) DEFAULT NULL,
  `content_text` text,
  `prayer_text` text,
  `further_study` varchar(512) DEFAULT NULL,
  `content_video_url` varchar(512) DEFAULT NULL,
  `content_audio_url` varchar(512) DEFAULT NULL,
  `content_pdf_url` varchar(512) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `language_id` (`language_id`),
  KEY `idx_category_topic` (`topic_id`),
  KEY `idx_category_parent` (`parent_id`),
  KEY `idx_category_date` (`year`,`month`,`day`),
  KEY `idx_category_age_group` (`age_group`)
) ENGINE=MyISAM AUTO_INCREMENT=524 DEFAULT CHARSET=latin1;
/*!40101 SET character_set_client = @saved_cs_client */;

