DROP TABLE IF EXISTS `tbl_topic`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!50503 SET character_set_client = utf8mb4 */;
CREATE TABLE `tbl_topic` (
  `id` int unsigned NOT NULL AUTO_INCREMENT,
  `slug` varchar(50) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT 'Unique identifier: rhapsody, foundation_school',
  `name` varchar(100) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT 'Display name',
  `description` text COLLATE utf8mb4_unicode_ci COMMENT 'Topic description',
  `image` varchar(512) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT 'Topic icon/image path',
  `topic_type` enum('daily','training') COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT 'daily' COMMENT 'daily=Rhapsody (time-based), training=Foundation School (self-paced)',
  `is_active` tinyint(1) NOT NULL DEFAULT '1',
  `row_order` int NOT NULL DEFAULT '0' COMMENT 'Display order',
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`),
  UNIQUE KEY `slug` (`slug`),
  KEY `idx_topic_slug` (`slug`),
  KEY `idx_topic_active` (`is_active`)
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
